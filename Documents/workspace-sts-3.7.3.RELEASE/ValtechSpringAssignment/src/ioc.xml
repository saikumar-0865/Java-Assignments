<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">
	
	<bean id="dontcallmedirectly" abstract="true" class="com.valtech.ioc.Point"></bean>
	<bean id="callMe" parent="dontcallmedirectly">
	</bean>
	
	<bean id="cp" class="com.valtech.ioc.Point">
	<constructor-arg index="0" value="120"></constructor-arg>
	<constructor-arg index="1" value="130"></constructor-arg>
	</bean>
	
	
	<bean id="pf" class="com.valtech.ioc.PointFactory"></bean> 
	
	<bean id="pointFromFactory" factory-bean="pf" factory-method="createpoint">
	<constructor-arg index="0" value="120"></constructor-arg>
	</bean>
	
	<!--<bean id="pf" class="com.valtech.ioc.PointFactory"></bean>  -->
	
	<bean id="Line1" class="com.valtech.ioc.Line" autowire="byName"></bean>
	
	
	<bean id ="line" class="com.valtech.ioc.Line" >
	<property name="start" ref="start"></property>
	<property name="end" ref="end"></property></bean>
	
	<bean id ="line1" class="com.valtech.ioc.Line" >
	<property name="start" ref="start"></property>
	<property name="end" ref="end"></property></bean>
	
	
	
	
	<bean id ="start" class="com.valtech.ioc.Point" scope="prototype">
	<property name="x" value="100"></property>
	<property name="y" value="200"></property>
	</bean>
	
	<!--  <bean id="p" name=""></bean> -->
	
	<bean id ="p" name="p1,p2,end" class="com.valtech.ioc.Point" scope="prototype">
	<property name="x" value="10"></property>
	<property name="y" value="20"></property>
	</bean>


</beans>
